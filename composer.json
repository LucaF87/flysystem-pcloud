{
    "name": "lucaf87/laravel-pcloud",
    "type": "project",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "LucaF87\\LaravelPCloud\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Luca Falbo",
            "email": "falbo.luca87@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload-dev": {
        "psr-4": {
            "LucaF87\\LaravelPCloud\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucaF87\\LaravelPCloud\\Providers\\CustomPCloudServiceProvider"
            ],
            "aliases": {
                "PCloud": "LucaF87\\LaravelPCloud\\Facades\\PCloud",
                "PCloudFolder": "LucaF87\\LaravelPCloud\\Facades\\PCloudFolder",
                "PCloudAdapter": "LucaF87\\LaravelPCloud\\Facades\\PCloudAdapter"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "nette/schema": "^1.2",
        "pcloud/pcloud-php-sdk": "3.*"
    },
    "require-dev": {
        "orchestra/testbench": "8.*",
        "phpunit/phpunit": "^9.5"
    }
}
